// Test GetBobWidth and GetBobHeight functions

code test:
    extern func CreateBob(descriptor_ptr: int, b: int) -> int;
    extern func GetBobWidth(handle: int) -> int;
    extern func GetBobHeight(handle: int) -> int;
    extern var bob_enemy1: int;

    proc main() -> int {
        var handle:int;
        var w:int;
        var h:int;

        // Create a bob
        handle = CreateBob(&bob_enemy1, 0);
        if (handle == -1) {
            return -1;  // Failed to create
        }

        // Get dimensions
        w = GetBobWidth(handle);
        h = GetBobHeight(handle);

        // Return width + height as a simple test
        return w + h;
    }
